    public void serialEvent(InputStreamReader entre)
    {
        this.entree = entre;
    }
    
    
    public void read()
    {
      try
      {
      
      }
      catch(IOException e)
      {
        System.err.println(e.getMessage());
      }
      catch(Exception e)
      {
        System.err.println(e.getMessage());
      }
    }
